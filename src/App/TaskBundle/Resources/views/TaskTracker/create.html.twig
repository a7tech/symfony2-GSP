{% extends 'AppTaskBundle:TaskTracker:layout.html.twig' %}
{% set nav_title = entity.id ? 'Task tracker edit' : 'Task tracker new' %}

{% block actions_main %}

    <form class="form-horizontal" action="" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <div class="form-group">
            <label class="control-label col-xs-3" for="button_form_submit"></label>
            <div class="col-xs-6">
                <input class="btn btn-default" type="submit" value="Save">
            </div>
        </div>
    </form>

{% endblock %}